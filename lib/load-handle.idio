
display* "This is load-handle.idio"

;%%vm-dis 1

expr := "display* \"This is an expr for load-handle\""

display* "expr: " expr

ish := open-input-string expr

load-handle ish

bad-expr := "display* \"This is an bad-expr for load-handle"

display* "bad-expr: " bad-expr

ish := open-input-string bad-expr

load-handle ish

cont-expr := "(false)"

display* "cont-expr: " cont-expr

ish := open-input-string cont-expr

load-handle ish

(symbols)
display* "load-handle.idio: done"
