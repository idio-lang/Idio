test {
  even-p :+ function (x) {
    or (null? x) (odd-p (pt x))
  }
  odd-p :+ function (x) {
    if (null? x) #f (even-p (pt x))
  }
  list (odd-p '(i i i i i)) (even-p '(i i i i i))
} '(#t #f)

